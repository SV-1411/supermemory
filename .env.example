
# Supermemory Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required: OpenRouter API key for AI chat
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Required: Pinecone API key for vector storage
PINECONE_API_KEY=your_pinecone_api_key_here

# Optional: OpenAI API key (if you want to use OpenAI instead of OpenRouter)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Anthropic API key (if you want to use Claude)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Customize storage path for local development
SUPERMEMORY_STORAGE_PATH=./data

# Optional: Customize embedding model
SUPERMEMORY_EMBEDDING_MODEL=Xenova/all-MiniLM-L6-v2
